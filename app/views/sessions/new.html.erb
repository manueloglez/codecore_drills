<h1>Sign In</h1>

<%= form_with url: session_path, local: true do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit "Sign In" %>
<% end %>


<div class="mt-4">
  <a>Forgot Password?</a>
  <span class="inline"> <%= link_to ' Dont have an account? Sign Up', new_user_path, class: "nav-link" %> </span>
</div>
